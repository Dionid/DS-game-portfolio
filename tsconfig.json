{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "target": "esnext",
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "strict": true,
    "isolatedModules": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "dvaApp": ["src/dvaApp/index.ts"],
      "models": ["src/models/index.ts"],
      "models/*": ["src/models/*"],
      "components/*": ["src/components/*"],
      "styles/*": ["src/styles/*"],
      "game": ["src/game/index.ts"],
      "game/*": ["src/game/*"]
    }
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}